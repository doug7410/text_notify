$("#individual-notification").html("<%= escape_javascript(render partial: 'notification_form' ) %>");
  

<% if @notification.errors.empty? %>
  $(".notification-alert").html("<%= @success_message %>");
  $('.notification-alert').effect("highlight", {}, 1500);
<% end %>

<% if params[:commit] == 'send later' %>
  $('#txt-queue').html("<%= escape_javascript(render partial: 'txt_queue' ) %>")
<% end %>

$.mask.definitions['~']='[+-]';
$('.input-mask-phone').mask('(999) 999-9999');



$("#customer").focus(function() {
  
  var ac_config = { 
    source: $('#customer').data('autocomplete-source'), 
    select: function(event, ui){ 
      $("#phone").val(ui.item.phone); 
      }, 
      minLength:1 
    };

  $("#customer").autocomplete(ac_config);
});  

$(".queue-alert").html("");
<% @notification.errors.clear %>
<% @customer.errors.clear %>
